// Baseado no exercício anterior, faça o mesmo processo
//sem fazer uso de uma terceira variável
algoritmo "semnome"

var
   nota1: real
   nota2: real
   nota3: real
   media: real
inicio
   escreva("Digite a 1° nota: ")
   leia(nota1)
   escreva("Digite a 2° nota: ")
   leia(nota2)

   escreva("O resultado é: ",(nota1-nota2)/10)
fimalgoritmo

