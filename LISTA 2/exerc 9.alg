algoritmo "semnome"

var
     n1: real


inicio

    escreval("Digite um número entre 10 e 45:")
    leia(n1)


    se (n1 >= 10) entao
       se n1 <= 45 entao
           escreva("Válido")
          senao
           escreva("Inválido")
          fimse
       senao
         escreva("Inválido")
    fimse

fimalgoritmo


